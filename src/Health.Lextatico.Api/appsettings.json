{
    "HealthChecks-UI": {
        "HealthChecks": [
            {
                "Name": "Gateway.Lextatico.Api",
                "Uri": "https://api.lextatico.com.br/healthchecks-data-ui"
            },
            {
                "Name": "Auth.Lextatico.Api",
                "Uri": "https://auth.lextatico.com.br/healthchecks-data-ui"
            },
            {
                "Name": "Account.Lextatico.Api",
                "Uri": "https://account.lextatico.com.br/healthchecks-data-ui"
            },
            {
                "Name": "Analyzer.Lextatico.Api",
                "Uri": "https://analyzer.lextatico.com.br/healthchecks-data-ui"
            }
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithEnvironmentName",
            "WithCorrelationId"
        ],
        "Properties": {
            "Application": "Health.Lextatico.Api"
        },
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    },
    "AllowedHosts": "*"
}
